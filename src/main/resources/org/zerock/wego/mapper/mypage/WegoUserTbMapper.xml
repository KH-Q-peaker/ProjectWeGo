<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="org.zerock.wego.mapper.mypage.WegoUserTbMapper">
  <select id="select">
 			SELECT *
 			FROM tuser_tb
 			where user_id= #{user_id}
  </select>
  <delete id="delete">
   			DELETE FROM tuser_tb WHERE user_id= #{user_id}
  </delete>
  <insert id="insert">
		INSERT INTO tuser_tb (user_id,nickname,social_id) VALUES(#{user_id},#{nickname},#{social_id})
	</insert>
  
  <update id="update">
 			UPDATE tuser_tb
			SET nickname = #{nickname},address=#{address}, san_range=#{san_range}, san_taste=#{san_taste},user_pic=#{user_pic},social_id=#{social_id}
			WHERE user_id=#{user_id}
  </update>
  
  
  </mapper>